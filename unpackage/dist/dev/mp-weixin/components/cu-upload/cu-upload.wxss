/**主题色*/
.cu-upload {
  display: flex;
  flex-wrap: wrap;
}
.cu-upload-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 220rpx;
  height: 220rpx;
  background-color: #f4f5f7;
  border-radius: 4rpx;
  margin-bottom: 15rpx;
}
.cu-upload-item {
  position: relative;
  margin: 0 15rpx 15px 0;
}
.cu-upload-item-delete {
  position: absolute;
  right: 0;
  top: 0;
  width: 14px;
  height: 14px;
  background: #373737;
  border-bottom-left-radius: 100px;
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: center;
}
.cu-upload-item-delete-icon {
  position: relative;
  bottom: 4px;
  left: 2px;
}