.cu-loading{display:flex;align-items:center;justify-content:center;margin:30rpx 0}.cu-loading-circle{width:48rpx;height:48rpx;border-radius:50%;border:4rpx solid #f1f1f1;border-top-color:#b7b7b7;animation:rotate 1.2s linear infinite}.cu-loading-text{font-size:28rpx;color:#606266;margin-left:16rpx}.cu-loading-line{height:0;width:100rpx;border:2rpx solid #e6e8eb}@keyframes rotate{0%{transform:rotate(0)}to{transform:rotate(1turn)}}
