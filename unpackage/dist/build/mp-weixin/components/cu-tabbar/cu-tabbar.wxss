.cu-tabbar{width:100%;position:fixed;bottom:0;left:0;right:0;display:flex;align-items:center;justify-content:space-around;background-color:#fff;z-index:9999;border-top:1rpx solid #dadbde}.cu-tabbar-item{width:100rpx;height:100rpx;display:flex;flex-direction:column;align-items:center;justify-content:center}.cu-tabbar-item text{font-size:26rpx;color:#7d7e80;margin-top:4rpx}.cu-tabbar-item.active text{color:#3d7eff!important}.cu-tabbar-round{width:100rpx;height:100rpx;border-radius:50%;position:absolute;top:-30rpx;left:0;background:#fff;z-index:-1;transition:all .5s;border-top:1rpx solid #dadbde}
