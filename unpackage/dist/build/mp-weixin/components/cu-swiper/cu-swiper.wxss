.cu-swiper{position:relative;width:690rpx;background-color:#dedede;border-radius:12rpx;margin:0 auto;overflow:hidden}.cu-swiper swiper{height:inherit!important}.cu-swiper-indicator{width:100%;display:flex;align-items:center;justify-content:center;position:absolute;bottom:20rpx;left:0}.cu-swiper-indicator-item{width:15rpx;height:15rpx;border-radius:50%;background-color:rgba(0,0,0,.19);transition:all .8s;margin:0 7rpx}.cu-swiper-indicator-item-active{width:45rpx;border-radius:8rpx;background-color:#fff}.cu-swiper-item{position:relative;width:100%;height:100%}.cu-swiper-item image{width:100%;height:100%}.cu-swiper-item-title{position:absolute;bottom:0;left:0;width:100%;background-color:rgba(0,0,0,.19);color:#fff;font-size:28rpx;padding:8rpx 18rpx;overflow:hidden;white-space:nowrap;text-overflow:ellipsis}
